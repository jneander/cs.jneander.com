{
  "name": "@jneander/sandbox",
  "version": "0.0.1",
  "description": "A place to exercise various projects together and dogfood their development environments",
  "main": "./index.js",
  "scripts": {
    "build": "NODE_ENV=development build-app --config ./config/app.js",
    "build:production": "NODE_ENV=production build-app --config ./config/app.js",
    "build:watch": "NODE_ENV=development build-app --config ./config/app.js --watch",
    "clean": "rimraf __build__",
    "deploy": "node ./scripts/deploy.js",
    "lint": "lint-code",
    "lint:fix": "lint-code --fix",
    "start:watch": "NODE_ENV=development build-app --config ./config/app.js --watch --serve",
    "test": "NODE_ENV=test run-specs --config config/specs.js",
    "test:watch": "NODE_ENV=test run-specs --config config/specs.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jneander/jneander"
  },
  "author": "Jeremy Neander <jeremy.neander@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jneander/jneander/issues"
  },
  "homepage": "https://github.com/jneander/jneander",
  "dependencies": {
    "@instructure/ui-container": "^5.5.0",
    "@instructure/ui-elements": "^5.5.0",
    "@instructure/ui-themes": "^5.5.0",
    "@instructure/ui-themeable": "^5.5.0",
    "normalize.css": "^8.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "@jneander/dev-tools": "^0.0.10",
    "chai": "^4.1.2",
    "npm-run-all": "^4.1.2",
    "sinon": "^4.4.1"
  },
  "private": true
}
